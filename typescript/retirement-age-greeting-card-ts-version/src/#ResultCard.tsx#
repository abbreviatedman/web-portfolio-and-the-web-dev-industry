import "./ResultCard.css";

const ResultCard = (props) => {
  const { firstName, lastName, ageAtEnd, futureValue } = props;

  return (
    <div className="card">
      <h2>Result</h2>
      <p>
        Name: {firstName} {lastName}
      </p>
      <p>Age at End of Saving Period: {ageAtEnd}</p>
      <p>Future Value of 401K (starting at $10,000): ${futureValue.toFixed(2)}</p>
    </div>
  );
};

export default ResultCard;
